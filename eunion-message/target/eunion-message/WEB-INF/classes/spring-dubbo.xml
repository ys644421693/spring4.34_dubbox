<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <bean id="messageManage" class="com.app.base.service.impl.MessageManageImpl"></bean>

    <dubbo:application name="eunion-message"></dubbo:application>

    <dubbo:registry address="zookeeper://127.0.0.1:2181"></dubbo:registry>

    <dubbo:protocol name="dubbo" port="20880"></dubbo:protocol>

    <dubbo:service interface="com.app.base.service.MessageManage" ref="messageManage"></dubbo:service>
</beans>